char	*p(char *dest, char *s)
{
	char buf[4104];

	puts(s);
	read(0, buf, 4096);
	strchr(buf, '\n') = 0;
	return strncpy(dest, buf, 20);
}

char	*pp(char *dest)
{
	//dest[42]
	char src[20];
	char v3[28];

	p(src, " - "); // src = A
	p(v3, " - "); // aA00aA01aA02aA03aA04
	strcpy(dest, src);
	dest[strlen(dest)] = 0x080486A4;
	return strcat(dest, v3);
}

int	main(int argc, const char **argv, const char **envp)
{
	char s[42];

	pp(s);
	puts(s);
	return 0;
}
